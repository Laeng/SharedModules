    },
    "PersonalInventory": {
      "class": "Cargo",
      "matches": [
        "hardpoint_personal_inventory.*",
        "hardpoint_personal_storage.*",
        "hardpoint_storage_%d",
        "hardpoint_general_cargo_%d+"
